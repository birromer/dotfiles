{
  "EqTikz": {
    "prefix": "eq-tikz",
    "body": [
      "\\begin{equation}",
      "\t\\begin{tikz-cd}",
      "\t\t$0",
      "\t\\end{tikz-cd}",
      "\t\\label{diag:$1}",
      "\\end{equation}"
    ],
    "description": "Add an equation surrounded tikz-cd diagram"
  },
  "Tikz": {
    "prefix": "tikz",
    "body": [
      "\\begin{tikz-cd}",
      "\t$0",
      "\\end{tikz-cd}"
    ],
    "description": "Add a tikz-cd diagram"
  },
  "Inline Math — \\( … \\)": {
    "prefix": "fm",
    "body": [
      "\\($TM_SELECTED_TEXT$1\\)"
    ],
    "description": "Inline Math"
  },
  "Inline Math — \\[ … \\]": {
    "prefix": "dm",
    "body": [
      "\\[",
      "$TM_SELECTED_TEXT$1",
      "\\]"
    ],
    "description": "Inline Math"
  },
  "Inkscape": {
    "prefix": "inkscape",
    "body": [
      "\\begin{figure}[H]",
      "\t\\begin{center}",
      "\t\t\\incfig{figures/$1.pdf_tex}",
      "\t\\end{center}",
      "\t\\caption{$3}\\label{fig:$4}",
      "\\end{figure}",
      "$0"
    ],
    "description": "Add a an inkscape (vector) figure"
  },
  "Label": {
    "prefix": "label",
    "body": [
      "\\label{$1}"
    ],
    "description": "Display a category"
  },
  "Category": {
    "prefix": "category",
    "body": [
      "\\cat{$1}"
    ],
    "description": "Display a category"
  },
  "Caligraphy": {
    "prefix": "cal",
    "body": [
      "\\mathcal{$1}$0"
    ],
    "description": "Display in math caligraphy style"
  },
  "Black board": {
    "prefix": "bb",
    "body": [
      "\\mathbb{$1}$0"
    ],
    "description": "Display in math black board style"
  },
  "Typewriter": {
    "prefix": "tt",
    "body": [
      "\\mathtt{$1}$0"
    ],
    "description": "Display in math typewriter style"
  },
  "Bold face": {
    "prefix": "bf",
    "body": [
      "\\textbf{$1}$0"
    ],
    "description": "Display in normal bold face style"
  },
  "Smallcaps": {
    "prefix": "sc",
    "body": [
      "\\textsc{$1}$0"
    ],
    "description": "Display in small caps style"
  },
  "Beamer": {
    "prefix": "t_beamer",
    "body": [
      "\\documentclass[bigger, 9pt]{beamer}",
      "\\usetheme{CambridgeUS}",
      "\\AtBeginSection[]{\\begin{frame}<beamer>\\frametitle{Presentat‌​‌​ion agenda}\\tableofcontents[currentsection]\\end{frame}}",
      "\\setbeamertemplate{blocks}[rounded][shadow=false]\n",
      "\\input{phd-beamer-header.tex}",
      "\\input{phd-macros.tex}",
      "\\addbibresource{../library.bib}\n",
      "\\title{$1}",
      "\\author{Bernardo Hummes Flores}",
      "\\date{\\today}\n",
      "\\begin{document}\n",
      "\\maketitle",
      "\\begin{frame}{Outline}",
      "\\tableofcontents",
      "\\end{frame}\n",
      "$0\n",
      "\\printbibliography",
      "\\end{document}"
    ],
    "description": "Add beamer template"
  },
  "Beamer columns": {
    "prefix": "columns",
    "body": [
      "\\begin{columns}",
      "\t\\begin{column}{0.$1\\columnwidth}",
      "\t\t$0",
      "\t\\end{column}",
      "\t\\begin{column}{0.$2\\columnwidth}",
      "\t\t$0",
      "\t\\end{column}",
      "\\end{columns}"
    ],
    "description": "Beamer columns"
  },
  "Beamer column": {
    "prefix": "column",
    "body": [
      "\\begin{column}{0.$1\\columnwidth}",
      "\t$0",
      "\\end{column}"
    ],
    "description": "Beamer columns"
  },
  "Beamer block": {
    "prefix": "block",
    "body": [
      "\\begin{block}{$1}",
      "\t$0",
      "\\end{block}"
    ],
    "description": "Beamer block"
  },
  "Beamer frame": {
    "prefix": "frame",
    "body": [
      "\\begin{frame}{$1}",
      "\t$0",
      "\\end{frame}"
    ],
    "description": "Beamer frame"
  },
  "Footnote": {
    "prefix": "footnote",
    "body": [
      "\\footnote{$1}"
    ],
    "description": "Add an inkscape (vector) figure"
  },
  "Import": {
    "prefix": "import",
    "body": [
      "\\zimport{./}{$1}"
    ],
    "description": "Import file correcting headers"
  },
  "Emphassis": {
    "prefix": "emph",
    "body": [
      "\\emph{$1}"
    ],
    "description": "Add emphasis markup"
  },
  "Notes": {
    "prefix": "t_note",
    "body": [
      "%! TEX root = ${TM_FILENAME}",
      "\\documentclass{note}",
      "\\title{$2} %TODO: Add to the index.",
      "\\date{${CURRENT_DATE}/${CURRENT_MONTH}/${CURRENT_YEAR}}",
      "\\author{Bernardo Hummes}\n",
      "\\begin{document}",
      "%─────Content────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────",
      "% TODO: Complete.\n$0\n",
      "%─────Links───────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────",
      "\\before",
      "\\after",
      "%─────End content────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────",
      "\\end{document}"
    ],
    "description": "Add note template"
  },
  "Literature note": {
    "prefix": "t_reference_note",
    "body": [
      "%! TEX root = ${TM_FILENAME}",
      "\\documentclass{note}",
      "\\title{$2} %TODO: Add to the index.",
      "\\date{${CURRENT_DATE}/${CURRENT_MONTH}/${CURRENT_YEAR}}",
      "\\author{Bernardo Hummes}\n",
      "\\begin{document}",
      "%─────Content────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────",
      "% TODO: Complete.\n$0\n",
      "%─────Links───────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────",
      "\\seealso",
      "%─────End content────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────",
      "\\end{document}"
    ],
    "description": "Add literature note template"
  },
  "Presentation": {
    "prefix": "t_presentation",
    "body": [
      "%! TEX root = ${TM_FILENAME}",
      "\\documentclass{presentation}",
      "\\title{$1} %TODO: Add to the index.",
      "\\date{${CURRENT_DATE}/${CURRENT_MONTH}/${CURRENT_YEAR}}",
      "\\author{Bernardo Hummes}\n",
      "\\begin{document}",
      "\\maketitle",
      "\\begin{frame}{Outline}",
      "\\tableofcontents",
      "\\end{frame}",
      "%─────Content────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────",
      "\\section{Context}",
      "\\begin{frame}{$2} % TODO: Complete.",
      "$0",
      "\\end{frame}",
      "%─────End content────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────",
      "\\begin{frame}[allowframebreaks]{References}",
      "\\printbibliography",
      "\\vfill",
      "\\end{frame}",
      "\\end{document}"
    ],
    "description": "Add presentation template"
  },
  "TODO": {
    "prefix": "todo",
    "body": [
      "\n%TODO: $1\n$0"
    ],
    "description": "Add todo comment"
  }
}
