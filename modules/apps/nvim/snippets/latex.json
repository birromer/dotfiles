  "Beamer": {
    "prefix": "t_beamer",
    "body": [
      "\\documentclass[bigger, 9pt]{beamer}",
      "\\usetheme{CambridgeUS}",
      "\\AtBeginSection[]{\\begin{frame}<beamer>\\frametitle{Presentat‌​‌​ion agenda}\\tableofcontents[currentsection]\\end{frame}}",
      "\\setbeamertemplate{blocks}[rounded][shadow=false]\n",
      "\\input{phd-beamer-header.tex}",
      "\\input{phd-macros.tex}",
      "\\addbibresource{../library.bib}\n",
      "\\title{$1}",
      "\\author{Bernardo Hummes Flores}",
      "\\date{\\today}\n",
      "\\begin{document}\n",
      "\\maketitle",
      "\\begin{frame}{Outline}",
      "\\tableofcontents",
      "\\end{frame}\n",
      "$0\n",
      "\\printbibliography",
      "\\end{document}"
    ],
    "description": "Add beamer template"
  },
  "Beamer columns": {
