      "\\begin{document}",
      "%─────Content────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────",
      "% TODO: Complete.\n$0\n",
      "%─────Links───────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────",
      "\\before",
      "\\after",
      "%─────End content────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────────",
      "\\end{document}"
    ],
    "description": "Add note template"
  },
