{
  "EqTikz": {
    "prefix": "eq-tikz",
    "body": [
      "\\begin{equation}",
      "\t\\begin{tikz-cd}",
      "\t\t$0",
      "\t\\end{tikz-cd}",
      "\t\\label{diag:$1}",
      "\\end{equation}"
    ],
    "description": "Add an equation surrounded tikz-cd diagram"
  },
  "Tikz": {
    "prefix": "tikz",
    "body": [
      "\\begin{tikz-cd}",
      "\t$0",
      "\\end{tikz-cd}"
    ],
    "description": "Add a tikz-cd diagram"
  },
  "Inline Math — \\( … \\)": {
    "prefix": "fm",
    "body": [
      "\\($TM_SELECTED_TEXT$1\\)"
    ],
    "description": "Inline math"
  },
  "Inline Math — \\[ … \\]": {
    "prefix": "dm",
    "body": [
      "\\[$TM_SELECTED_TEXT$1\\]"
    ],
    "description": "Display math"
  },
  "Label": {
