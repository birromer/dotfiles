{
  "EqTikz": {
    "prefix": "eq-tikz",
    "body": [
      "\\begin{equation}",
      "\t\\begin{tikz-cd}",
      "\t\t$0",
      "\t\\end{tikz-cd}",
      "\t\\label{diag:$1}",
      "\\end{equation}"
    ],
    "description": "Add an equation surrounded tikz-cd diagram"
  },
  "Tikz": {
    "prefix": "tikz",
    "body": [
      "\\begin{tikz-cd}",
      "\t$0",
      "\\end{tikz-cd}"
    ],
    "description": "Add a tikz-cd diagram"
  },
  "Inline Math — \\( … \\)": {
    "prefix": "fm",
    "body": [
      "\\($TM_SELECTED_TEXT$1\\)"
    ],
    "description": "Display Math"
  },
  "Inkscape": {
    "prefix": "inkscape",
    "body": [
      "\\begin{figure}[H]",
      "\t\\begin{center}",
      "\t\t\\incfig{figures/$1.pdf_tex}",
      "\t\\end{center}",
      "\t\\caption{$3}\\label{fig:$4}",
      "\\end{figure}",
      "$0"
    ],
    "description": "Add a an inkscape (vector) figure"
  },
  "Category": {
    "prefix": "category",
    "body": [
      "\\cat{$1}"
    ],
    "description": "Add a an inkscape (vector) figure"
  }
}
